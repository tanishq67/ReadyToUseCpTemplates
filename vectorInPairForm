vpll get_vec_in_pform(vl& s){
        vpll v;
        ll i = 1, cnt = 1;
        for(; i < s.size(); i++){
            if(s[i] == s[i-1])
                cnt++;
            else{
                v.pb({s[i-1],cnt});
                cnt = 1;
            }
        }
        v.pb({s[n-1],cnt});
        return v;
}
